{
  "expo": {
    "name": "SupaCharts",
    "slug": "supacharts",
    "version": "2.0",
    "icon": "./assets/images/icon.png",
    "scheme": "sbcharts",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#222B45",
    "platforms": [
      "android",
      "ios",
      "web"
    ],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#222B45"
    },
    "updates": {
      "fallbackToCacheTimeout": 5000,
      "enabled": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "2",
      "supportsTablet": true,
      "bundleIdentifier": "com.ljacobs.charts",
      "appStoreUrl": "https://apps.apple.com/app/apple-store/id1612680145?pt=117925864&ct=aj&mt=8",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "twitter"
        ]
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "playStoreUrl": "https://play.google.com/store/apps/details?id=rich.alex.charts",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#222B45"
      },
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "rich.alex.charts",
      "versionCode": 6
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "build": {
        "babel": {
          "include": [
            "@ui-kitten/components"
          ]
        }
      }
    },
    "plugins": [
      "@config-plugins/android-jsc-intl"
    ],
    "extra": {
      "eas": {
        "projectId": "a0bec425-4ba2-4deb-845d-e9d6a78d285c"
      }
    },
    "owner": "lohanjacobs"
  }
}
