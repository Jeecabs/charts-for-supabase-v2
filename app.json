{
  "expo": {
    "name": "Charts",
    "slug": "sbcharts",
    "version": "2.0",
    "icon": "./assets/images/icon.png",
    "scheme": "sbcharts",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#222B45",
    "platforms": ["android", "ios", "web"],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#222B45"
    },
    "updates": {
      "fallbackToCacheTimeout": 5000,
      "enabled": true
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "1",
      "supportsTablet": true,
      "bundleIdentifier": "com.mrarich.charts",
      "appStoreUrl": "https://apps.apple.com/app/apple-store/id1612680145?pt=117925864&ct=aj&mt=8",
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["twitter"]
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "playStoreUrl": "https://play.google.com/store/apps/details?id=rich.alex.charts",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#222B45"
      },
      "permissions": ["READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"],
      "package": "rich.alex.charts",
      "versionCode": 5
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "build": {
        "babel": {
          "include": ["@ui-kitten/components"]
        }
      }
    },
    "extra": {
      "eas": {
        "projectId": "d2681c33-6cce-498c-8f3d-3048a29c6050"
      }
    },
    "plugins": ["@config-plugins/android-jsc-intl"]
  }
}
